{
  "name": "agent-graph",
  "version": "0.1.1",
  "description": "A functional programming library for agent graph operations",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "bun build ./src/index.ts --outdir ./dist --target node --format esm",
    "build:cjs": "bun build ./src/index.ts --outdir ./dist --target node --format cjs --outfile index.cjs",
    "build": "bun run clean && bun run build:types && bun run build:js && bun run build:cjs",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "agent",
    "graph",
    "functional",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
